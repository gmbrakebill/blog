<%-include("partials/header")-%> 

<h1>Home</h1>

<p><%= startingContent %> </p>


<% posts.forEach(post => {%>

    <h1><%= post.title%></h1>
    <% if(post.body.length > 100) {%>
    
    <p><%= post.body.substring(0,100)+"..."%> <a href ="/posts/<%=post.title%>">Read More</a> </p>


     <%}else { %>
    <p><%= post.body %></p>
    <% } %>
    
    <%});%>
    





<%-include("partials/footer")-%> 